{
  "algorithm": {
    "name": "random",
    "params": {
      "abstraction": true,
      "combine_obs_state": false,
      "string_propagation": true,
			"alpha": 2,
      "alpha_0": 128,
      "delay": 1,
      "delta": 0.2,
      "epsilon": 0.2,
      "mu": 0.14,
      "n": 10,
      "nb_episodes": 2000000,
      "sampling_policy": "RandomPolicy",
      "termination_schedule": 1000,
      "use_vmax": true,
      "m_0": 10000
    }
  },
  "environment": {
    "name": "rotating_mab",
    "params": {
      "specification": {
        "nb_arms": 5,
        "reward_win": 100,
        "win_probs": [
          0.9,
          0.2,
          0.2,
          0.2,
          0.2
        ]
      },
      "markovian": false,
      "use_stop_prob": true,
      "flicker_prob": 0.0,
      "nb_steps": 10
    }
  },
  "evaluation": {
    "episodes": 50,
    "frequency": 15000,
    "steps": 10
  },
  "log-path": "experiments/logs/rotating_mab_k_5_random_baseline/2/logs",
  "log-frequency": 50000,
	"seed": 2
}