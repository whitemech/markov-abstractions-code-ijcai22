{
  "algorithm": {
    "name": "random",
    "params": {
      "abstraction": true,
      "combine_obs_state": false,
      "string_propagation": true,
			"alpha": 2,
      "alpha_0": 128,
      "delay": 1,
      "delta": 0.2,
      "epsilon": 0.2,
      "mu": 0.224,
      "n": 70,
      "nb_episodes": 3000001,
      "sampling_policy": "RandomPolicy",
      "termination_schedule": 10000,
      "use_vmax": false,
      "m_0": 10000
    }
  },
  "environment": {
    "name": "grid",
    "params": {
      "specification": {
        "goal_position": [
          3,
          4
        ],
        "m": 8,
        "n": 8,
        "prob_success_move": 1.0,
        "reward_goal": 100 
      },
      "markovian": false,
      "use_stop_prob": true,
      "flicker_prob": 0.2,
      "nb_steps": 10
    }
  },
  "evaluation": {
    "episodes": 50,
    "frequency": 15000,
    "steps": 10
  },
  "log-path": "experiments/logs/grid_random_baseline/1/logs",
  "log-frequency": 50000,
	"seed": 1
}