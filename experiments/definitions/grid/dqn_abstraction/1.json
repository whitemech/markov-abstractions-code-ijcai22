{
  "algorithm": {
    "name": "dqn",
    "params": {
      "abstraction": true,
      "combine_obs_state": true,
      "string_propagation": true,
			"alpha": 2,
      "alpha_0": 128,
      "delay": 1,
      "delta": 0.2,
      "epsilon": 0.2,
      "mu": 0.224,
      "n": 70,
      "nb_episodes": 3000001,
      "sampling_policy": "RandomPolicy",
      "termination_schedule": 10000,
      "use_vmax": false,
      "m_0": 10000,
      "net_arch": [516, 256, 64],
      "buffer_size": 1000,
      "batch_size": 128,
      "learning_rate": 1e-3,
      "learning_starts": 5000,
      "target_update_interval": 500,
      "train_freq": 100,
      "exploration_final_eps": 0.05,
      "exploration_fraction": 0.1,
      "total_timesteps": 10000000000,
      "log_interval": 100,
      "verbose": 1
    }
  },
  "environment": {
    "name": "grid",
    "params": {
      "specification": {
        "goal_position": [
          3,
          4
        ],
        "m": 8,
        "n": 8,
        "prob_success_move": 1.0,
        "reward_goal": 100 
      },
      "markovian": false,
      "use_stop_prob": true,
      "flicker_prob": 0.2,
      "nb_steps": 10
    }
  },
  "evaluation": {
    "episodes": 50,
    "frequency": 15000,
    "steps": 10
  },
  "log-path": "experiments/logs/grid_dqn_abstraction/1/logs",
  "log-frequency": 50000,
	"seed": 1
}